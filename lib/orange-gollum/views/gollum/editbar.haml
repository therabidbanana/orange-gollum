      / Gollum Links
      .page.gollum.links
        %table{:border => "0", :cellpadding => "0", :cellspacing => "0", :width => "100%"}
          %tr
            %th Description
            %th What you type
            %th What you get
          %tr
            %td Internal link
            %td
              %pre [[Page Title]]
              %pre [[Link Label|Page Title]]
            %td
              %a{:href => "Page-Title"} Page Title
              %br/
              %a{:href => "Page-Title"} Link Label
          %tr
            %td External link
            %td
              %pre [[http://example.com]]
              %pre [[Example|http://example.com]]
            %td
              %a{:href => "http://example.com"} http://example.com
              %br/
              %a{:href => "http://example.com"} Example
          %tr
            %td File link
            %td
              %pre [[Absolute PDF|/images/example.pdf]]
              %pre [[Relative PDF|example.pdf]]
            %td
              %a{:href => "/images.example.pdf"} Absolute PDF
              %br/
              %a{:href => "/example.pdf"} Relative PDF
          %tr
            %td Escaped link
            %td
              %pre '[[Page Title]]
            %td
              [[Page Title]]
