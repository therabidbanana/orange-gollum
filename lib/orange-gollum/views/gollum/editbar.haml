#editbar
  .menu
    .group.group-separator
      %span.link.button{:title => "Link"} Link
      %span.image.button{:title => "Image"} Image
    .group.group-separator
      %span.bold.button{:title => "Bold"} Bold
      %span.italic.button{:title => "Italic"} Italic
    .group.group-separator
      %span.ul.button{:title => "Unordered List"} Unordered List
      %span.ol.button{:title => "Ordered List"} Ordered List
    .group
      %span.tab.help
        %a{:href => "#"} Help
    .group.group-right
      %span.tab.format
        Format:
        %select#wiki_format{:name => "format"}
          %option{:selected => "true", :value => "markdown"} Markdown
